<h1>ランキング</h1>
    <% @posts.each do |post| %>
        <div class="posts-contents">
          <%# <%= image_tag @post.fish_image.url %> %>
          <p>
          <%# <%= image_tag "#{post.fish_image}" ,class:"card-img-top" %> %>
          <%= post.fish_name %>
          <%= post.catch_size %>
          <%= post.weight %>
          <%= post.lure %>
          <%= post.rod %>
          <%= post.reel %>
          <%= post.line %>
          <%= post.range %>
          </p>
        </div>
    <% end %>



